pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'

                archiveArtifacts artifacts: 'C:\WorkspaceJava\CICDWithJenkins\target\CICDWithJenkins-1.0-SNAPSHOT.jar', fingerprint: true
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'

                junit '**/target/*.xml'
            }
        }

    }
}